{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "exclude": [
    "src/**/__tests__/*"
  ],
  "include": [
    "env.d.ts",
    "src/components/**/*",
    "src/components/**/*.vue"
  ],
  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}